{
	"extends": [
		"plugin:@wordpress/eslint-plugin/recommended",
		"plugin:jest/recommended"
	],
	"env": {
	  "browser": true,
	  "es6": true
	},
  "settings": {
    "react": {
      "version": "16.2.0"
    }
  },
  "globals": {
    "React": true,
    "googlesitekit": true,
    "googlesitekitAdminbar": true,
    "googlesitekitDashboard": true,
    "googlesitekitSettings": true,
    "lodash": true,
    "googlesitekitCurrentModule": true,
    "gtag": true,
    "process": true
  },
  "overrides": [
    {
      "files": [ "tests/e2e/**/*.js" ],
      "env": {
        "browser": true
      },
      "globals": {
        "browser": true,
        "page": true,
        "wp": true
      }
    }
  ]
}
